#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,num;
	setbuf(stdout,NULL);
	printf("enter a number");
	scanf("%d",&num);
	for(i=0;i<=num;i++){
		printf("Hello");
		if(i==5){
			break;
		}

		printf("Hai");

		}
	printf("Program finished");


	return EXIT_SUCCESS;
}
