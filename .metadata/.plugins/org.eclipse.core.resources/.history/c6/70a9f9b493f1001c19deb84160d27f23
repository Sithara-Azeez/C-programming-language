#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int grade;
	setbuf(stdout,NULL);
	printf("enter your grade");
	scanf("%d",&grade)
	if(grade==<90){
		printf("your grade is A");
	}

		return EXIT_SUCCESS;
}
