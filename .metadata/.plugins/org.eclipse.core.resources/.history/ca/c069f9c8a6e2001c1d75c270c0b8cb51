#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int a1[100],a2[100],i,limit,j,temp;
	setbuf(stdout,NULL);
	printf("enter array limit");
	scanf("%d",&limit);
	printf("enter a values of Array1:");

	for(i=0;i<limit;i++){

		scanf("%d",&a1[i]);
	}
	printf("enter a values of Array2:");
	for(i=0;i<limit;i++){
		scanf("%d",&a2[i]);
	}

		temp=a1[i];
		a1[i]=a2[i];
		a2[2]=temp;

	printf("Array1: ");

	for(i=0;i<limit;i++){
		printf("%d\t",a1[i]);
	}
	printf("Array2: ");

	for(i=0;i<limit;i++){
		printf("%d\t",a2[i]);
	}


	return EXIT_SUCCESS;
}
