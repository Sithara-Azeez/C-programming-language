#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int grade;
	setbuf(stdout,NULL);
	printf("enter your grade");
	scanf("%d",&grade);
	if{
		printf("your grade is A");
	}else if{
		grade=80-89;
		printf("your grade id B");
	}

		return EXIT_SUCCESS;
}
