#include <stdio.h>
#include <stdlib.h>

int main(void) {
    float mark;
	setbuf(stdout,NULL);
	printf("ENTER YOUR MARK:");
	scanf("%f",&mark);
	if(mark>=50){
		printf("passed");
	}else if(mark<=100){
		printf("Mark is out of 100 Only");
	}
	else{
		printf("SORRY FAILED!!!");
	}
	return EXIT_SUCCESS;
}
