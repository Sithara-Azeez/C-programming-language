#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num;
	printf("enter your grade");
	scanf("%d",&num);
	if(num>50){
		printf("you've passed");
	}else{
		printf("you're failed");
	}
		return EXIT_SUCCESS;
}
