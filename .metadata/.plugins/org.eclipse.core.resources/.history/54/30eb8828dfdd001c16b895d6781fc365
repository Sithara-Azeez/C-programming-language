#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,num,flag=0;
	setbuf(stdout,NULL);
	printf("enter a number");
	scanf("%d",&num);
	for(i=2;i<=num/2;i++){
		if(i%2==0){
			flag=1;
			break;
		}

	}
	if(flag==0){
		printf("Prime Number");
	}else{
		printf("Not Prime Number");
	}

	return EXIT_SUCCESS;
}
