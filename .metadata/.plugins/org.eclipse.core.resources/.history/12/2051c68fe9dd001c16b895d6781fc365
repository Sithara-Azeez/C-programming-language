#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,limit=10,num,result;
	setbuf(stdout,NULL);
	printf("enter a number");
	scanf("%d",&num);
	for(i=1;i<=limit;i++){

		result=i*num;
	}
	printf("Result = %d",result);


	return EXIT_SUCCESS;
}
