#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,num,j;
	setbuf(stdout,NULL);
	printf("enter a number");
	scanf("%d",&num);
	for(i=1;i<=num;i++){
		for(j=0;j<i;j--){
			printf("*");
		}
		printf("\n");
	}


	return EXIT_SUCCESS;
}
