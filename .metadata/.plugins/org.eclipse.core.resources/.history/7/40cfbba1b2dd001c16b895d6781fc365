#include <stdio.h>
#include <stdlib.h>

int main(void) {
    float mark=100;
	setbuf(stdout,NULL);
	printf("ENTER YOUR MARK:");
	scanf("%f",&mark);
	if(mark>=50){
		printf("passed");
	}
	else{
		printf("SORRY FAILED!!!");
	}
	return EXIT_SUCCESS;
}
