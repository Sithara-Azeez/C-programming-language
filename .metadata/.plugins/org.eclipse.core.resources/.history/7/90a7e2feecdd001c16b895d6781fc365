#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int limit,i,flag=0,sum=0;
	setbuf(stdout,NULL);
	printf("enter a limit");
	scanf("%d",&limit);
	for(i=1;i<=limit;i++){
		if(i%2==0){
			flag=1;
			break;
		}
		if(flag==0){
				sum=sum+i;
				printf("Result=%d",sum);
			}
	}



	return EXIT_SUCCESS;
}
